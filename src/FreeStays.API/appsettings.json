{
  "ConnectionStrings": {
    "DefaultConnection": "",
    "Redis": ""
  },
  "JwtSettings": {
    "Secret": "",
    "Issuer": "FreeStays.API",
    "Audience": "FreeStays.Client",
    "ExpirationInMinutes": 60,
    "RefreshTokenExpirationInDays": 7
  },
  "Hangfire": {
    "DashboardPath": "/hangfire",
    "WorkerCount": 2,
    "MaxJobHistoryCount": 500,
    "MaxRetryAttempts": 2,
    "AutomaticRetryAttempts": 1,
    "FailedListSize": 1000,
    "DeletedListSize": 2000,
    "SucceededListSize": 2000,
    "AdminEmail": "admin@freestays.com"
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "https://freestays.eu",
      "https://freestays-frontend-xi1vzy-ed390a-3-72-175-63.traefik.me",
      "https://shorttrips.eu"
    ]
  },
  "FileUpload": {
    "BasePath": "wwwroot/uploads",
    "MaxFileSizeInMB": 5,
    "AllowedExtensions": [
      ".jpg",
      ".jpeg",
      ".png",
      ".gif",
      ".webp"
    ],
    "BaseUrl": "/uploads"
  },
  "AppSettings": {
    "BaseUrl": "https://api.freestays.com"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/freestays-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 30
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  },
  "AllowedHosts": "*"
}